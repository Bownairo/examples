type TokenBalance = 
 record {
   amount: nat;
   "principal": principal;
   token: Token;
 };
type Token = text;
type OrderPlacementResponse = 
 record {
   order: Order;
   status: text;
 };
type Order = 
 record {
   from: Token;
   fromAmount: nat;
   id: text;
   owner: principal;
   to: Token;
   toAmount: nat;
 };
type CancelOrderResponse = 
 record {
   order_id: text;
   status: text;
 };
service : {
  balances: () -> (vec TokenBalance) query;
  cancel_order: (text) -> (CancelOrderResponse);
  check_order: (text) -> (opt Order);
  convert_icp: () -> (opt text);
  convert_token: () -> (opt text);
  deposit: () -> () oneway;
  deposit_address: () -> (blob);
  init: () -> (opt text);
  list_order: () -> (vec Order) query;
  place_order: (Token, nat, Token, nat) -> (OrderPlacementResponse);
  whoami: () -> (principal) query;
  withdraw: () -> () oneway;
}
